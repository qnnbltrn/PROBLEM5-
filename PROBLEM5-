#include <iostream>
#include <conio.h>

using namespace std;

int main()
{
     int multplr = 1, n = 1;
    
    while (n <= 30)
    {
        cout << n;
        n = n + multplr;
        
        if (n >= 10)
        {
            multplr = 2;
        }
        
        if (n <= 30)
        {
            cout << ",";
        }
    }
_getch (); 
return 0;
}
